{
    "name": "@symbio/devstack",
    "version": "1.0.0-canary.2",
    "description": "SYMBIO Next.js Devstack",
    "repository": {
        "type": "git",
        "url": "https://github.com/SYMBIO/next-devstack"
    },
    "license": "MIT",
    "types": "types/app.ts",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "build": "rimraf dist && NODE_ENV=production babel -d dist --ignore node_modules,types ./",
        "dev": "npm run build",
        "prepublish": "npm run build && npm run types",
        "relay": "relay-compiler",
        "test": "jest",
        "test:watch": "jest --watch",
        "types": "tsc --declaration --emitDeclarationOnly --declarationDir dist",
        "typescript": "tsc --noEmit --declaration"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.12.0",
        "axios": "^0.21.1",
        "core-js": "^3.11.2",
        "datocms-client": "^3.4.7",
        "dayjs": "^1.10.4",
        "graphql": "^15.5.0",
        "hls.js": "^1.0.2",
        "html-react-parser": "^1.2.6",
        "html-to-text": "^7.1.1",
        "next": "^10.2.0",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-relay": "^11.0.2",
        "relay-runtime": "^11.0.2",
        "slugify": "^1.5.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.14.0",
        "@graphql-codegen/cli": "^1.21.4",
        "@graphql-codegen/introspection": "1.18.2",
        "@graphql-codegen/schema-ast": "^1.18.2",
        "@types/hls.js": "^1.0.0",
        "@types/node": "^15.0.1",
        "@types/react": "^16.14.5",
        "@types/react-dom": "^16.9.12",
        "@types/react-relay": "^11.0.1",
        "@types/relay-runtime": "^11.0.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-relay": "^11.0.2",
        "dotenv": "^8.2.0",
        "relay-compiler": "^11.0.2",
        "relay-compiler-language-typescript": "^14.0.0",
        "relay-config": "^11.0.2",
        "rimraf": "^3.0.2"
    }
}
