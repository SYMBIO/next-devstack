{
    "description": "SYMBIO Next.js Devstack",
    "repository": {
        "type": "git",
        "url": "https://github.com/SYMBIO/next-devstack"
    },
    "license": "MIT",
    "scripts": {
        "build": "env-cmd --silent graphql-codegen --config codegen.yml && npm run relay && NODE_ENV=production next build",
        "build-storybook": "build-storybook -c .storybook -o ./public/storybook --quiet",
        "create-component": "node node_modules/@symbio/headless/dist/scripts/createComponent.js $F $N",
        "create-story": "node node_modules/@symbio/headless/dist/scripts/createStory.js $F $N",
        "dev": "next dev",
        "export": "NODE_ENV=production next export",
        "generate-blocks": "env-cmd node node_modules/@symbio/headless/dist/scripts/createBlocks.js && npm run relay",
        "generate-models": "env-cmd -f .env node node_modules/@symbio/headless/dist/scripts/generateModels.js",
        "graphql-codegen": "env-cmd -f .env graphql-codegen --config codegen.yml",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "relay": "relay-compiler",
        "start": "NODE_ENV=production next start",
        "storybook": "start-storybook --no-dll",
        "test": "jest",
        "test:watch": "jest --watch",
        "update-strings": "env-cmd -f .env node node_modules/@symbio/headless/dist/scripts/updateStrings.js"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@react-google-maps/api": "^2.2.0",
        "@symbio/cms": "^1.0.0-canary.64",
        "@symbio/cms-datocms": "^1.0.0-canary.64",
        "@symbio/headless": "^1.0.0-canary.64",
        "axios": "^0.21.1",
        "busboy": "^0.3.1",
        "clsx": "^1.1.1",
        "core-js": "^3.17.1",
        "dayjs": "^1.10.6",
        "formik": "^2.2.9",
        "graphql-tag": "^2.12.5",
        "hls.js": "^1.0.10",
        "html-react-parser": "^1.2.8",
        "html-to-text": "^8.0.0",
        "mandrill-api": "^1.0.45",
        "next": "^11.1.2",
        "next-pwa": "^5.3.1",
        "path-to-regexp": "^6.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-image-lightbox": "^5.1.4",
        "react-relay": "^12.0.0",
        "react-responsive-carousel": "^3.2.21",
        "relay-runtime": "^12.0.0",
        "slugify": "^1.6.0",
        "uuid": "^8.3.2",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@graphql-codegen/cli": "^2.1.1",
        "@graphql-codegen/introspection": "2.1.0",
        "@graphql-codegen/schema-ast": "^2.1.0",
        "@next/bundle-analyzer": "^11.1.2",
        "@storybook/addon-a11y": "^6.3.7",
        "@storybook/addon-actions": "^6.3.7",
        "@storybook/addon-backgrounds": "^6.3.7",
        "@storybook/addon-console": "^1.2.3",
        "@storybook/addon-controls": "^6.3.7",
        "@storybook/addon-docs": "^6.3.7",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-storysource": "^6.3.7",
        "@storybook/addon-viewport": "^6.3.7",
        "@storybook/addons": "^6.3.7",
        "@storybook/client-api": "^6.3.7",
        "@storybook/react": "^6.3.7",
        "@svgr/webpack": "^5.5.0",
        "@tailwindcss/line-clamp": "^0.2.1",
        "@types/busboy": "^0.2.4",
        "@types/hls.js": "^1.0.0",
        "@types/html-to-text": "^8.0.1",
        "@types/jest": "^27.0.1",
        "@types/mandrill-api": "^1.0.30",
        "@types/node": "^16.7.10",
        "@types/object-fit-images": "^3.2.2",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/uuid": "^8.3.1",
        "@types/yup": "^0.29.13",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "@welldone-software/why-did-you-render": "^6.2.1",
        "autoprefixer": "^10.3.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-relay": "^12.0.0",
        "dotenv": "^10.0.0",
        "env-cmd": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.1",
        "jest": "^27.1.0",
        "lint-staged": "^11.1.2",
        "postcss": "^8.3.6",
        "prettier": "^2.3.2",
        "react-docgen-typescript-loader": "^3.7.2",
        "relay-compiler": "^12.0.0",
        "relay-compiler-language-typescript": "^14.1.0",
        "relay-config": "^12.0.0",
        "sass": "^1.39.0",
        "sass-loader": "^10.2.0",
        "storybook-addon-next-router": "^3.0.8",
        "tailwindcss": "^2.2.9",
        "ts-jest": "^27.0.5",
        "typescript": "^4.4.2"
    },
    "babel": {
        "presets": [
            "next/babel"
        ],
        "plugins": [
            "relay"
        ]
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": true
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "react",
            "@typescript-eslint",
            "prettier"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:prettier/recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 2018,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "rules": {},
        "settings": {
            "react": {
                "version": "detect"
            }
        }
    }
}
