{
    "name": "@symbio/cms-datocms",
    "version": "1.0.0-canary.27",
    "description": "SYMBIO Next.js Devstack",
    "repository": {
        "type": "git",
        "url": "https://github.com/SYMBIO/next-devstack"
    },
    "license": "MIT",
    "types": "types/app.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "build": "rimraf dist && babel -d dist -x .ts,.tsx,.js --ignore dist,node_modules,types ./",
        "dev": "concurrently \"tsc --watch --declaration --emitDeclarationOnly --declarationDir dist\" \"babel -d dist -x .ts,.tsx,.js --ignore dist,node_modules,types --watch ./\"",
        "graphql-codegen": "env-cmd -f ../../.env graphql-codegen --config codegen.yml",
        "prepublish": "npm run build && npm run types",
        "relay": "relay-compiler",
        "test": "jest",
        "test:watch": "jest --watch",
        "types": "cpy types/* dist/types && tsc --declaration --emitDeclarationOnly --declarationDir dist",
        "typescript": "tsc --noEmit --declaration"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.13.0",
        "axios": "^0.21.1",
        "core-js": "^3.14.0",
        "datocms-client": "^3.4.10",
        "dayjs": "^1.10.5",
        "graphql": "^15.5.0",
        "hls.js": "^1.0.6",
        "html-react-parser": "^1.2.6",
        "html-to-text": "^7.1.1",
        "next": "^10.2.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "relay-runtime": "^11.0.2",
        "slugify": "^1.5.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.14.3",
        "@graphql-codegen/cli": "^1.21.5",
        "@graphql-codegen/introspection": "1.18.2",
        "@graphql-codegen/schema-ast": "^1.18.3",
        "@symbio/cms": "1.0.0-canary.27",
        "@symbio/headless": "1.0.0-canary.27",
        "@types/hls.js": "^1.0.0",
        "@types/node": "^15.12.2",
        "@types/react": "^17.0.9",
        "@types/react-dom": "^17.0.6",
        "@types/relay-runtime": "^11.0.0",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "babel-loader": "^8.2.2",
        "babel-plugin-relay": "^11.0.2",
        "concurrently": "^6.2.0",
        "cpy-cli": "^3.1.1",
        "dotenv": "^10.0.0",
        "relay-compiler": "^11.0.2",
        "relay-compiler-language-typescript": "^14.0.0",
        "relay-config": "^11.0.2",
        "rimraf": "^3.0.2"
    },
    "gitHead": "2c0aa85fa5fb6a6ed7a7adea0590ba7b16940228"
}
