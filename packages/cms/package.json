{
    "name": "@symbio/cms",
    "version": "1.0.0-canary.18",
    "description": "Abstract adapter for headless CMS for SYMBIO devstack",
    "repository": {
        "type": "git",
        "url": "https://github.com/SYMBIO/next-devstack"
    },
    "license": "MIT",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "build": "rimraf dist && babel -d dist --ignore dist,node_modules,types ./",
        "dev": "concurrently \"tsc --watch --declaration --emitDeclarationOnly --declarationDir dist\" \"babel -d dist --ignore dist,node_modules,types --watch ./\"",
        "prepublish": "npm run build && npm run types",
        "test": "jest",
        "test:watch": "jest --watch",
        "types": "tsc --declaration --emitDeclarationOnly --declarationDir dist",
        "typescript": "tsc --noEmit --declaration"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.14.0",
        "@types/node": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-relay": "^11.0.2",
        "concurrently": "^6.0.2",
        "dotenv": "^8.2.0",
        "rimraf": "^3.0.2"
    }
}
