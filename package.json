{
  "description": "SYMBIO Next.js Devstack",
  "repository": {
    "type": "git",
    "url": "https://github.com/SYMBIO/next-devstack"
  },
  "license": "MIT",
  "scripts": {
    "build": "env-cmd --silent graphql-codegen --config codegen.yml && npm run relay && NODE_ENV=production next build",
    "build-storybook": "build-storybook -c .storybook -o ./public/storybook --quiet",
    "create-blocks": "env-cmd node scripts/createBlocks.js && npm run relay",
    "dev": "next dev",
    "export": "NODE_ENV=production next export",
    "graphql-codegen": "env-cmd graphql-codegen --config codegen.yml",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "relay": "relay-compiler",
    "start": "NODE_ENV=production next start",
    "storybook": "start-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "update-strings": "env-cmd node scripts/updateStrings.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "@react-google-maps/api": "^1.9.7",
    "axios": "^0.19.2",
    "busboy": "^0.3.1",
    "core-js": "^3.6.5",
    "datocms-client": "^3.0.34",
    "formik": "^2.1.4",
    "graphql": "^15.3.0",
    "hls.js": "^0.14.1",
    "html-react-parser": "^0.13.0",
    "html-to-text": "^5.1.1",
    "mandrill-api": "^1.0.45",
    "moment-timezone": "^0.5.31",
    "next": "^9.4.4",
    "next-pwa": "^3.0.3",
    "object-fit-images": "^3.2.4",
    "path-to-regexp": "^6.1.0",
    "react": "^16.13.1",
    "react-datocms": "^1.2.5",
    "react-dom": "^16.13.1",
    "react-image-lightbox": "^5.1.1",
    "react-intersection-observer": "^8.26.1",
    "react-relay": "^9.1.0",
    "react-responsive-carousel": "^3.1.57",
    "redis": "^3.0.2",
    "relay-hooks": "^3.5.1",
    "relay-runtime": "^9.1.0",
    "sass": "^1.26.10",
    "slugify": "^1.4.4",
    "uuid": "^8.2.0",
    "xml": "^1.0.1",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/introspection": "1.17.0",
    "@graphql-codegen/schema-ast": "^1.17.0",
    "@graphql-codegen/typescript": "1.17.0",
    "@graphql-codegen/typescript-operations": "1.17.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/client-api": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@svgr/webpack": "^5.4.0",
    "@types/busboy": "^0.2.3",
    "@types/hls.js": "^0.13.0",
    "@types/html-to-text": "^5.1.1",
    "@types/jest": "^26.0.4",
    "@types/mandrill-api": "^1.0.30",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.0.23",
    "@types/object-fit-images": "^3.2.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-relay": "^7.0.8",
    "@types/redis": "^2.8.25",
    "@types/relay-runtime": "^9.1.5",
    "@types/uuid": "^8.0.0",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-relay": "^9.1.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "moment-timezone-data-webpack-plugin": "^1.3.0",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-config": "^9.1.0",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "relay"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {},
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
